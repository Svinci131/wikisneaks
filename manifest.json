{
  "manifest_version": 2,

  "name": "wikipedia enhancer",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "content_scripts": [
    {
      "run_at":"document_start",
     "matches": ["http://www.google.com/*", "https://en.wikipedia.org/wiki/*"],
      "js": ["./libraries/axios.js", "./libraries/xml2json.js",  "./domFunctions/imageFunctions.js", "./domFunctions/numberChanging.js", "clarifai.js", "searchImages.js", "main.js"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_title": "wikipedia enhancer",
    "default_icon": "./imgs/rabbit (1).png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://www.googleapis.com/*",
    "tabs",
    "bookmarks",
    "https://www.google.com/search/*",
    "http://www.blogger.com/",
    "http://*.google.com/",
    "unlimitedStorage"
  ]
}
